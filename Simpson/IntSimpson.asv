clc
clear all
% Définir la fonction
f= inline ('1./((x.*x)+1)','x');
%Définir les bornes de l'intervalle
a=0;
b=2;
n=4;
h=(b-a)/n;
x=[a:h:b]
f(x)
I1=0;
I2=0;
for i=1:1:n
    m=mod(i,2);
    if (m==1) 
    I1=I1+2*f(i);
    else
    I2=I2+4*f(i);
    end

end
I=(h/3)*(I1+I2+f()